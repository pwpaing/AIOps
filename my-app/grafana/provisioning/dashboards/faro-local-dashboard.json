      "title": "Faro Manual Test Logs",
      "gridPos": {
        "h": 14,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "datasource": {
        "type": "loki",
        "uid": "cfdc385m938qoe"
      },
      "targets": [
        {
          "expr": "{source=\"faro\"} |= \"Manual Faro test log\"",
          "refId": "A"
        }
      ],
      "options": {
        "showLabels": true,
        "showTime": true,
        "sortOrder": "Descending",
        "wrapLogMessage": true,
        "enableLogDetails": true
      }
    },
    {
      "id": 2,
