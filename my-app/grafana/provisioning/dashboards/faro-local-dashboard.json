        "h": 8,
        "w": 24,
        "x": 0,
        "y": 14
      },
      "datasource": {
        "type": "loki",
        "uid": "cfdc385m938qoe"
      },
      "targets": [
        {
          "expr": "sum(count_over_time({source=\"faro\"} |= \"Manual Faro test log\" [1m]))",
          "legendFormat": "logs/min",
          "refId": "A"
